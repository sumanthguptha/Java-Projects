package com.zensar.training.bean;

import java.util.List;

public class StringAdderRunnable implements Runnable{
	
	List<String> list;

	@Override
	public void run() {
		list.add("Mumbai");
		try {
			Thread.sleep(2000);
		}catch(InterruptedException e) {
			e.printStackTrace();
		}
		list.add("Delhi")
	}

}
